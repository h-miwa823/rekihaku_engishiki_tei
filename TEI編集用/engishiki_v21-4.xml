                        <head ana="神代三陵"/>
                        <p ana="項"
                            corresp="engishiki_en.xml#item21400101 engishiki_ja.xml#item21400101"
                            xml:id="item21400101"><table>
                                <row>
                                    <cell rendition="#width170"
                                        ><anchor/>日<anchor/>向<anchor/>埃<anchor/>山陵</cell>
                                    <cell>〈</cell>
